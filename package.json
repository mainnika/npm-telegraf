  },
  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "telegraf": "^3.6.7",
    "ts-node": "^2.1.2",
    "tslint": "^4.5.1",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.2.2",
    "typings": "^2.1.0"
  }
